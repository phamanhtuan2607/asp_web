@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <p>Phạm Anh Tuấn</p>
    <div>
        <h2>Bảng cửu chương</h2>
        @for (int i = 1; i <= 9; i++)
        {
            <div>
                <h3>Cửu chương @i</h3>
                @for (int j = 1; j <= 10; j++)
                {
                    <p>@i x @j = @(i * j)</p>
                }
            </div>
        }
    </div>
</div>
